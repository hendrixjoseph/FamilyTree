<facelet-taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
                version="2.2">
    <description>Custom UI Compentents made by Joseph Hendrix</description>
    <namespace>http://hendrix11.wright.edu/facelets</namespace>
    <short-name>hendrix11</short-name>
    <tag>
        <tag-name>menu</tag-name>
        <source>resource/tag/menu.xhtml</source>
        <attribute>
            <name>currentView</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>editPersonDialog</tag-name>
        <source>resource/tag/editPersonDialog.xhtml</source>
        <attribute>
            <name>header</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>widgetVar</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>person</name>
            <required>true</required>
            <type>edu.wright.hendrix11.familyTree.entity.Person</type>
        </attribute>
        <attribute>
            <name>isNewPerson</name>
            <required>true</required>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>googleMapLink</tag-name>
        <source>resource/tag/googleMapLink.xhtml</source>
        <attribute>
            <name>place</name>
            <required>true</required>
            <type>edu.wright.hendrix11.familyTree.entity.place.Place</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>personLink</tag-name>
        <source>resource/tag/personLink.xhtml</source>
        <attribute>
            <name>person</name>
            <required>true</required>
            <type>edu.wright.hendrix11.familyTree.entity.Person</type>
        </attribute>
        <attribute>
            <name>rendered</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>childTable</tag-name>
        <source>resource/tag/childTable.xhtml</source>
        <attribute>
            <name>children</name>
            <required>true</required>
            <type>java.util.List</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>pageNav</tag-name>
        <source>resource/tag/pageNav.xhtml</source>
        <attribute>
            <name>dataBean</name>
            <required>true</required>
            <type>edu.wright.hendrix11.familyTree.dataBean.DataBean</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>pageSort</tag-name>
        <source>resource/tag/pageSort.xhtml</source>
        <attribute>
            <name>dataBean</name>
            <required>true</required>
            <type>edu.wright.hendrix11.familyTree.dataBean.DataBean</type>
        </attribute>
        <attribute>
            <name>text</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>sort</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>chart</tag-name>
        <component>
            <component-type>org.primefaces.component.Chart</component-type>
            <renderer-type>edu.wright.hendrix11.familyTree.faces.CustomChartRenderer</renderer-type>
        </component>
        <attribute>
            <description><![CDATA[Unique identifier of the component in a namingContainer.]]></description>
            <name>id</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[Boolean value to specify the rendering of the component, when set to false component will not be rendered.]]></description>
            <name>rendered</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <description><![CDATA[An el expression referring to a server side UIComponent instance in a backing bean.]]></description>
            <name>binding</name>
            <required>false</required>
            <type>javax.faces.component.UIComponent</type>
        </attribute>
        <attribute>
            <description><![CDATA[Name of the client side widget.]]></description>
            <name>widgetVar</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[Type of the chart.]]></description>
            <name>type</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[Model of the chart.]]></description>
            <name>model</name>
            <required>false</required>
            <type>org.primefaces.model.chart.ChartModel</type>
        </attribute>
        <attribute>
            <description><![CDATA[Inline style of the component.]]></description>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[Style class of the component.]]></description>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description><![CDATA[In responsive mode, chart is redrawn when window is resized.]]></description>
            <name>responsive</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
    </tag>
</facelet-taglib>
