<facelet-taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
                version="2.2">
    <description>Custom SVG UI Compentents made by Joseph Hendrix</description>
    <namespace>http://hendrix11.wright.edu/svg</namespace>
    <short-name>svg</short-name>
    <tag>
        <tag-name>barChart</tag-name>
        <component>
            <component-type>edu.wright.hendrix11.svg.jsf.barChart.BarChartComponent</component-type>
            <renderer-type>edu.wright.hendrix11.svg.jsf.barChart.BarChartRenderer</renderer-type>
        </component>
        <attribute>
            <name>chartModel</name>
            <required>true</required>
            <type>edu.wright.hendrix11.svg.jsf.barChart.BarChartModel</type>
        </attribute>
        <attribute>
            <name>height</name>
            <required>true</required>
            <type>java.lang.Number</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>scatterPlot</tag-name>
        <component>
            <component-type>edu.wright.hendrix11.svg.jsf.scatterPlot.ScatterPlotComponent</component-type>
            <renderer-type>edu.wright.hendrix11.svg.jsf.scatterPlot.ScatterPlotRenderer</renderer-type>
        </component>
        <attribute>
            <name>chartModel</name>
            <required>true</required>
            <type>edu.wright.hendrix11.svg.jsf.scatterPlot.ScatterPlotModel</type>
        </attribute>
        <attribute>
            <name>width</name>
            <required>true</required>
            <type>java.lang.Number</type>
        </attribute>
        <attribute>
            <name>height</name>
            <required>true</required>
            <type>java.lang.Number</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>