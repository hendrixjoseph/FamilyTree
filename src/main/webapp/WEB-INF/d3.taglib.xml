<facelet-taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
                version="2.2">
    <description>C3 / D3 UI Compentents implemented by Joseph Hendrix</description>
    <namespace>http://hendrix11.wright.edu/d3</namespace>
    <short-name>d3</short-name>
    <tag>
        <tag-name>chart</tag-name>
        <component>
            <component-type>edu.wright.hendrix11.d3.chart.ChartComponent</component-type>
            <renderer-type>edu.wright.hendrix11.d3.chart.ChartRenderer</renderer-type>
        </component>
        <attribute>
            <name>chartModel</name>
            <required>true</required>
            <type>edu.wright.hendrix11.d3.chart.ChartModel</type>
        </attribute>
        <attribute>
            <name>id</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>type</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>gridX</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>gridY</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>showLegend</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <name>legendPosition</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>title</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
    <tag>
        <tag-name>wordCloud</tag-name>
        <component>
            <component-type>edu.wright.hendrix11.d3.wordCloud.WordCloudComponent</component-type>
            <renderer-type>edu.wright.hendrix11.d3.chart.WordCloudRenderer</renderer-type>
        </component>
        <attribute>
            <name>id</name>
            <required>true</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>width</name>
            <required>true</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>height</name>
            <required>true</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>padding</name>
            <required>false</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <name>font</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>
